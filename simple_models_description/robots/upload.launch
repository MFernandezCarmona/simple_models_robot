<launch>
  <arg name="laser_model"/>
  <arg name="robot"/>

  <!-- Robot description -->
  <param name="robot_description"
      command="$(find xacro)/xacro.py --inorder
              '$(find simple_models_description)/robots/robot.urdf.xacro'
    laser_model:=$(arg laser_model)
    robot:=$(arg robot)"/>

</launch>

